{
  "name": "template-vue-3",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run --coverage",
    "sonar": "vitest run --coverage && npx sonar-scanner -D sonar.login=%SONAR_TEMPLATE%",
    "test:e2e": "vite build && start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "vite build && start-server-and-test preview http://127.0.0.1:5050/ 'cypress run --browser chrome'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@vee-validate/i18n": "^4.8.3",
    "@vee-validate/rules": "^4.8.4",
    "axios": "^1.3.4",
    "chart.js": "^4.4.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.1",
    "keycloak-js": "^21.0.2",
    "pnpm": "^9.12.2",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primevue": "3.26.1",
    "vee-validate": "^4.8.4",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-istanbul": "^0.29.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.13.0",
    "cypress": "^12.9.0",
    "eslint": "^8.37.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-vue": "^9.10.0",
    "jsdom": "^21.1.1",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "sonar-scanner": "^3.1.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "~5.0.3",
    "vite": "^4.2.1",
    "vite-plugin-pwa": "^0.14.7",
    "vitest": "^0.29.8",
    "vue-router-mock": "^0.2.0",
    "vue-tsc": "^1.2.0",
    "workbox-precaching": "^6.5.4",
    "workbox-window": "^6.5.4"
  }
}
